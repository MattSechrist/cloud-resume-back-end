{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:GetObject",
            "Resource": "arn:aws:s3:::www.matthewsechrist.cloud/*",
            "Condition": {
                "StringEquals": {
                    "${{ secrets.REFERER_KEY }}": "${{ secrets.REFERER_VALUE }}"
                }
            }
        }
    ]
}
